{
    "name": "horde/horde-autoloader-cache",
    "description": "Cache for the Horde_Autoloader library",
    "type": "library",
    "homepage": "https://pear.horde.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "developer"
        }
    ],
    "version": "2.1.3",
    "time": "2017-04-11",
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.3",
        "pear-pear.horde.org/Horde_Autoloader": "^2@stable",
        "ext-json": "*"
    },
    "suggest": {
        "pear-pear.horde.org/horde_lz4": "*",
        "pear-pecl.php.net/lzf": "^1.5.2@stable",
        "pear-pecl.php.net/msgpack": "*",
        "ext-apc": "*",
        "ext-eaccelerator": "*",
        "ext-xcache": "*"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Autoloader_Cache": "2.*",
        "pear-horde/Horde_Autoloader_Cache": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Autoloader_Cache": "lib/"
        }
    }
}
