{
    "name": "horde/autoloader-cache",
    "description": "Caching library for Horde_Autoloader",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Autoloader_Cache",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        },
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "developer"
        }
    ],
    "version": "2.1.4",
    "time": "2021-02-13",
    "repositories": [
        {
            "type": "pear",
            "url": "https://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.3 || ^7",
        "pear-pear.horde.org/Horde_Autoloader": "^2",
        "ext-json": "*"
    },
    "suggest": {
        "pear-pear.horde.org/horde_lz4": "*",
        "pear-pecl.php.net/lzf": "^1.5.2",
        "pear-pecl.php.net/msgpack": "*",
        "ext-apc": "*",
        "ext-eaccelerator": "*",
        "ext-xcache": "*"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Autoloader_Cache": "2.*",
        "pear-horde/Horde_Autoloader_Cache": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Autoloader_Cache": "lib/"
        }
    }
}